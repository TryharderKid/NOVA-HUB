loveTab:AddButton("Te Quiero", function()
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Para Brigitte",
        Text = "¡Pensando en ti con una sonrisa!",
        Duration = 5
    })
end)

end)
